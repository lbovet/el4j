<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

    <!-- Version: $Revision$ -->
    <!-- URL: $URL$ -->
    <!-- Date: $Date$ -->
    <!-- Author: $Author$ -->

    <import
        resource="classpath:scenarios/common/remotingtests-jaxws-protocol-config.xml" />

    <!-- Default JAX-WS Setup -->
    <bean id="calculator"
        class="ch.elca.el4j.services.remoting.RemotingProxyFactoryBean">
        <property name="remoteProtocol">
            <ref bean="jaxwsProtocol" />
        </property>
        <property name="serviceInterface">
        <!-- use ...service.gen.CalculatorWS if no proxies should be generated -->
            <value>
                ch.elca.el4j.tests.remoting.service.CalculatorJaxws
            </value>
        </property>
        <property name="serviceName">
            <value>Calculator.Jaxws.RemotingtestsJaxws</value>
        </property>
    </bean>
</beans>


