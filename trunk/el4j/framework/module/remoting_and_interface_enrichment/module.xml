<?xml version="1.0"?>
<!-- project definition -->
<ant:project name="el4j" xmlns="antlib:ch.elca.el4ant" xmlns:ant="antlib:org.apache.tools.ant">
    <module name="module-remoting_and_interface_enrichment">
        <attribute name="set" value="framework-modules"/>
        <attribute name="binrelease.version" value="1.2"/>
        <dependency module="module-core"/>
        
        <!-- Hessian protocol -->
        <dependency jar="hessian-3.0.13.jar"/>

        <!-- Burlap protocol -->
        <dependency jar="burlap-2.1.12.jar"/>

        <!-- Soap protocol (Axis is used) -->
        <dependency jar="jaxrpc-1.1.jar"/>
        <dependency jar="saaj-1.2.jar"/>
        <dependency jar="javamail-1.3.2.jar"/>
        <dependency jar="wsdl4j-1.5.1.jar"/>
        <dependency jar="axis-1.2.1.jar"/>
        <dependency jar="axis-ant-1.2.1.jar"/>
        <dependency jar="commons-discovery-0.2.jar"/>
        <dependency jar="commons-httpclient-3.0-rc2.jar"/>
        <dependency jar="activation-1.0.2.jar"/>
        <dependency jar="servlet-api-2.4.jar"/>
    </module>
</ant:project>
