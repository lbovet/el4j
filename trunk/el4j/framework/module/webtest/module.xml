<?xml version="1.0"?>
<ant:project name="el4j" xmlns="antlib:ch.elca.el4ant" 
xmlns:ant="antlib:org.apache.tools.ant">
    <module name="module-webtest">
        <attribute name="set" value="framework-modules"/>
        <attribute name="binrelease.version" value="1.0-alpha1"/>
        <attribute name="junit.runnable" value="true"/>

        <dependency module="module-web"/>

        <!-- Core Libraries for JWebUnit Testing -->
        <dependency jar="jwebunit-core-1.3-rc1.jar"/>
        <dependency jar="jwebunit-htmlunit-plugin-1.3-rc1.jar"/>

        <!-- Libraries used by JWebUnit -->
        <dependency jar="htmlunit-1.8.jar"/>
        <dependency jar="regexp-1.3.jar"/>
        <dependency jar="xml-apis-1.2.1.jar"/>
        <dependency jar="js-1.5R4.1.jar"/>
        <dependency jar="nekohtml-0.9.5.jar"/>
        <dependency jar="xercesImpl-2.6.2.jar"/>
        <dependency jar="commons-httpclient-3.0.1.jar"/>
        <dependency jar="commons-io-1.1.jar"/>
        <dependency jar="commons-codec-1.3.jar"/>
        <dependency jar="jaxen-1.1-beta-8.jar"/>

    </module>
</ant:project>
