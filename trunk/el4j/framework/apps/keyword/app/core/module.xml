<?xml version="1.0"?>
<!-- project definition -->
<ant:project name="el4j" xmlns="antlib:ch.elca.el4ant" 
    xmlns:ant="antlib:org.apache.tools.ant">

    <module name="keyword-core">
        <!-- Working set -->
        <attribute name="set" value="keyword-modules"/>
        
        <!-- Binary version -->
        <attribute name="binrelease.version" value="1.2"/>
        
        <!-- Commons attributes enabled -->
        <attribute name="commons-attributes.enable" value="true"/>
        
        <!-- Module dependencies -->
        <dependency module="module-core"/>
    </module>
    
</ant:project>
