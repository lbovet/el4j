<?xml version="1.0"?>
<!-- Used binary modules. -->
<ant:project name="helloworld-binary_modules" xmlns="antlib:ch.elca.el4ant"
    xmlns:ant="antlib:org.apache.tools.ant">
    
    <!--
        Binary modules. The modules below are all from the EL4J 0.9.3 framework.
        Hello World modules can have dependencies to them like they were local
        modules. The name of the modules are the name of the attribute elements 
        below without the substring "binrelease.version.".
    -->
    <plugin name="binrelease.versions">
        <attribute name="binrelease.version.module-core" 
            value="1.3"/>
        <attribute name="binrelease.version.module-ibatis" 
            value="1.0"/>
        <attribute name="binrelease.version.module-hibernate" 
            value="1.0"/>
        <attribute name="binrelease.version.module-daemon_manager" 
            value="1.2"/>
        <attribute name="binrelease.version.module-env" 
            value="1.1"/>
        <attribute name="binrelease.version.module-exception_handling" 
            value="1.0"/>
        <attribute name="binrelease.version.module-jmx" 
            value="1.2"/>
        <attribute name="binrelease.version.module-light_statistics" 
            value="1.0"/>
        <attribute name="binrelease.version.module-remoting_and_interface_enrichment" 
            value="1.2"/>
        <attribute name="binrelease.version.module-remoting_ejb" 
            value="1.0"/>
        <attribute name="binrelease.version.module-security" 
            value="1.0"/>
        <attribute name="binrelease.version.module-spring_rcp" 
            value="1.1"/>
        <attribute name="binrelease.version.module-web" 
            value="1.2"/>
        <attribute name="binrelease.version.common-database" 
            value="1.1"/>
    </plugin>
    
</ant:project>
