<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <!-- $Version$ -->
    <!-- $URL$ -->
    <!-- $Date$ -->
    <!-- $Author$ -->
    
    <bean id="daoRegistry"
    	class="ch.elca.el4j.services.persistence.generic.dao.impl.DefaultDaoRegistry">
    	<property name="daos">
			<map>
				<entry key="ch.elca.el4j.applications.keyword.dom.Keyword">
					<ref bean="keywordDao"/>
				</entry>
				<entry key="ch.elca.el4j.applications.refdb.dom.Link">
					<ref bean="linkDao"/>
				</entry>
				<entry key="ch.elca.el4j.applications.refdb.dom.FormalPublication">
					<ref bean="formalPublicationDao"/>
				</entry>
				<entry key="ch.elca.el4j.applications.refdb.dom.Book">
					<ref bean="bookDao"/>
				</entry>
				<entry key="ch.elca.el4j.applications.refdb.dom.Annotation">
					<ref bean="annotationDao"/>
				</entry>
				<entry key="ch.elca.el4j.applications.refdb.dom.File">
					<ref bean="fileDao"/>
				</entry>
				<entry key="ch.elca.el4j.applications.refdb.dom.FileDescriptorView">
					<ref bean="fileDescriptorViewDao"/>
				</entry>
			</map>
		</property>	
    </bean>
  
</beans>