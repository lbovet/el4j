<?xml version="1.0"?>
<ant:project name="el4j" xmlns="antlib:ch.elca.el4ant"
    xmlns:ant="antlib:org.apache.tools.ant">

    <module name="module-spring_rcp">
        <!-- Working set -->
        <attribute name="set" value="framework-modules"/>

        <!-- Binary version -->
        <attribute name="binrelease.version" value="1.3-rc0"/>

        <!-- Module dependencies -->
        <dependency module="module-core"/>


        <!-- Spring rich client platform (rpc) libraries -->
        <!-- Spring RCP itself -->
        <dependency jar="spring-richclient-full-0.2.0-el4j-20060618.jar"/>

        <!-- Spring extra and sandbox modules -->
        <dependency jar="spring-binding-0.2005-07-22.jar"/>
        <dependency jar="spring-modules-sandbox-0.1-20060304.jar"/>

        <!-- Components -->
        <dependency jar="forms-1.0.5.jar"/>
        <dependency jar="looks-1.3.2.jar"/>
        <dependency jar="glazedlists-1.6.0_java15.jar"/>
        <dependency jar="swingx-0.2005-06-02.jar"/>
        <dependency jar="jdnc-0.7-all.jar"/>
        <dependency jar="microba-0.4.1.jar"/>

        <!-- Utils -->
        <dependency jar="concurrent-oswego-edu-2004-07-10.jar"/>
        <dependency jar="javahelp-2.0.jar"/>

        <!-- Docking -->
        <dependency jar="flexdock-0.4.0.jar"/>

        <!-- Others -->
        <dependency jar="commons-codec-1.2.jar"/>
        <dependency jar="commons-beanutils-1.7.0.jar"/>
    </module>

</ant:project>