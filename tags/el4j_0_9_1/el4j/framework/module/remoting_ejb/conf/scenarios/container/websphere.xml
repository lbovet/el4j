<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
    "http://www.springframework.org/dtd/spring-beans.dtd">
<!--
    Spring bean configuration file for jndi environment support in websphere.
-->
<beans>
    <!-- Version: $Revision$ -->
    <!-- Source: $Source$ -->
    <!-- Date: $Date$ -->
    <!-- Author: $Author$ -->
    
	<bean name="websphereJndiConfig" class="ch.elca.el4j.util.codingsupport.Properties">
	    <property name="properties">
	        <props>
				<prop key="java.naming.factory.initial">com.ibm.websphere.naming.WsnInitialContextFactory</prop>
				<prop key="ch.elca.el4j.jndi.prefix">iiop://</prop>
				<prop key="java.naming.factory.url.pkgs">com.ibm.ws.naming</prop>
			</props>
	    </property>
	</bean>
</beans>
