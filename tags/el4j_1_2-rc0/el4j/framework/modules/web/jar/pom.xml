	<!-- Version: $Revision$ -->
	<!-- URL: $URL$ -->
	<!-- Date: $Date$ -->
	<!-- Author: $Author$ -->


	<parent>
		<groupId>ch.elca.el4j.modules</groupId>
		<artifactId>el4j-framework-module-web</artifactId>
