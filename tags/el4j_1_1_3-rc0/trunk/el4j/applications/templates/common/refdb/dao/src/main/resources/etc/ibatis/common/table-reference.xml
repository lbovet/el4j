<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE sqlMap
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
	
<sqlMap namespace="refdb-core">
    <!-- Version: $Revision$ -->
    <!-- URL: $URL$ -->
    <!-- Date: $Date$ -->
    <!-- Author: $Author$ -->

    <!-- 
        Reference related invocations
    -->
    
    <delete id="deleteReference">
        delete from REFERENCESTABLE where KEYID=#key# AND
        OPTIMISTICLOCKINGVERSION=#optimisticLockingVersion#
    </delete>
    
    <delete id="deleteReferenceById" parameterClass="int">
        delete from REFERENCESTABLE where KEYID=#value#
    </delete>
</sqlMap>
