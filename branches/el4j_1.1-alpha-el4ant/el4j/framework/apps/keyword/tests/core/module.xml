<?xml version="1.0"?>
<ant:project name="el4j" xmlns="antlib:ch.elca.el4ant" 
    xmlns:ant="antlib:org.apache.tools.ant">

    <module name="keyword-core-tests">
        <!-- Working set -->
        <attribute name="set" value="application-tests"/>
        
        <!-- Enables JUnit -->
        <attribute name="junit.runnable" value="true"/>
        
        <!-- Module dependencies -->
        <dependency module="keyword-core">
            <mapping target="implementation"/>
        </dependency>
        <dependency module="common-database"/>
        <dependency module="module-env"/>
    </module>
    
</ant:project>
